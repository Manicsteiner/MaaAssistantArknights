[
    {
        "name_template": "MAA-{VERSION}-win-{PLATFORM}",
        "type": "MaaBundle",
        "configuration": {
            "include": [
                "**"
            ]
        }
    },
    {
        "name_template": "MAAComponent-DebugSymbol-{VERSION}-win-{PLATFORM}",
        "type": "MaaDebugSymbol",
        "configuration": {
            "include": [
                "**/*.pdb"
            ]
        }
    },
    {
        "name_template": "FakeOTA-ProgramOnly-{VERSION}-win-{PLATFORM}",
        "type": "MaaOTA",
        "configuration": {
            "include": [
                "**/MAA.exe",
                "**/MaaCore.dll"
            ]
        }
    }
]